    <DataSet Name="UnhealthyDB">
      <Query>
        <DataSourceName>dsDMDev</DataSourceName>
        <CommandText>
select di.InstanceName
        ,di.Environment
        ,ud.DatabaseName
        ,ud.DatabaseState
        ,CASE
            WHEN di.Environment = 'Prod' THEN 1
            WHEN di.Environment = 'Test' THEN 2
            WHEN di.Environment = 'Dev'  THEN 3
            WHEN di.Environment = 'DR'   THEN 4
            ELSE 5
        END AS GroupOrder
from Sys_Admin.RunBook.UnhealthyDatabases ud
    INNER JOIN Sys_Admin.RunBook.DatabaseInstances di
        ON di.InstanceName = ud.InstanceName
            AND di.LastCheckDateTime = ud.CheckDateTime</CommandText>
      </Query>
      <Fields>
        <Field Name="InstanceName">
          <DataField>InstanceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Environment">
          <DataField>Environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseName">
          <DataField>DatabaseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseState">
          <DataField>DatabaseState</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GroupOrder">
          <DataField>GroupOrder</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Alerts">
      <Query>
        <DataSourceName>dsDMDev</DataSourceName>
        <CommandText>
SELECT di.InstanceName
        ,di.Environment
        ,a.AlertName 
        ,a.AlertDateTime
        ,'Message Id: ' + CAST(a.MessageId as VARCHAR(100)) + ', Severity: ' + CAST(a.Severity AS VARCHAR(100)) AS ErrorNbr
        ,CASE
            WHEN di.Environment = 'Prod' THEN 1
            WHEN di.Environment = 'Test' THEN 2
            WHEN di.Environment = 'Dev'  THEN 3
            WHEN di.Environment = 'DR'   THEN 4
            ELSE 5
        END AS GroupOrder
from Sys_Admin.RunBook.Alerts a
    INNER JOIN Sys_Admin.RunBook.DatabaseInstances di
        ON di.InstanceName = a.InstanceName
            AND di.LastCheckDateTime = a.CheckDateTime</CommandText>
      </Query>
