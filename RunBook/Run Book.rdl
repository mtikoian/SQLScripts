        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Backups">
      <Query>
        <DataSourceName>dsDMDev</DataSourceName>
        <CommandText>
select di.InstanceName
        ,di.Environment
        ,fb.DatabaseName
        ,fb.LastFullBackup
        ,fb.LastDiffBackup
        ,CASE
            WHEN di.Environment = 'Prod' THEN 1
            WHEN di.Environment = 'Test' THEN 2
            WHEN di.Environment = 'Dev'  THEN 3
            WHEN di.Environment = 'DR'   THEN 4
            ELSE 5
        END AS GroupOrder
from Sys_Admin.RunBook.FailedBackups fb
    INNER JOIN Sys_Admin.RunBook.DatabaseInstances di
        ON di.InstanceName = fb.InstanceName
            AND di.LastCheckDateTime = fb.CheckDateTime</CommandText>
      </Query>
